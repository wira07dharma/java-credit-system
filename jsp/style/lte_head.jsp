  .table-form tbody td { padding: 0 !important; margin: 0 !important; }
  .table-form tbody td .form-control { height: 36px; }
  .table-form th span { margin-top: 2px !important; display: inline-block !important; width: 100% !important; }
  .table-form td span { padding: 9px; display: inline-block; }
  span.money { text-align: right; width: 100%; }
  .listgentitle div { font-weight: bold; }
  .listgensell .btn-edit.btn-sm, .listgensellOdd .btn-edit.btn-sm, .command a { display: inline-block !important; background: green; width: auto; padding:5px; text-align: left; color: white !important; border-radius: 3px; }
